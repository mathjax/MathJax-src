#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/regjsparser@0.9.1/node_modules/regjsparser/bin/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/regjsparser@0.9.1/node_modules/regjsparser/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/regjsparser@0.9.1/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/regjsparser@0.9.1/node_modules/regjsparser/bin/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/regjsparser@0.9.1/node_modules/regjsparser/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/regjsparser@0.9.1/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../regjsparser/bin/parser" "$@"
else
  exec node  "$basedir/../../../regjsparser/bin/parser" "$@"
fi
