#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/@babel+parser@7.23.3/node_modules/@babel/parser/bin/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/@babel+parser@7.23.3/node_modules/@babel/parser/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/@babel+parser@7.23.3/node_modules/@babel/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/@babel+parser@7.23.3/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/@babel+parser@7.23.3/node_modules/@babel/parser/bin/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/@babel+parser@7.23.3/node_modules/@babel/parser/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/@babel+parser@7.23.3/node_modules/@babel/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/@babel+parser@7.23.3/node_modules:/home/sorge/git/MathJax/MathJax-src/tests/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../../../../@babel/parser/bin/babel-parser.js" "$@"
fi
